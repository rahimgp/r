    <script>
        // Office Use মডাল দেখানোর ফাংশন
        function showOfficeUseModal() {
            const password = prompt("Please enter the password:");
            if (password === 'mpll321') {
                document.getElementById('officeUseModal').style.display = 'block';
            } else {
                alert("Incorrect password.");
            }
        }

        // Office Use মডাল বন্ধ করার ফাংশন
        function closeOfficeUseModal() {
            document.getElementById('officeUseModal').style.display = 'none';
        }
    </script>
