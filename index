    font-weight: bold;
    border-radius: 8px; /* Rounded corners */
    box-shadow: 0 4px 8px rgba(0, 0, 0, 0.2); /* Drop shadow for popup effect */
    opacity: 0; /* Initially invisible */
    z-index: 1000; /* Keep it on top of other content */
    animation: popup-animation 10s ease-in-out infinite; /* Popup effect with zoom */
  }

  .popup-text a {
    color: #FFFF00; /* Yellow text color */
    text-decoration: none; /* Removes underline from links */
  }

  .popup-text ul:hover {
    animation-play-state: paused; /* Pause on hover */
  }

  @keyframes popup-animation {
    0% {
      transform: translateX(-50%) scale(0.5); /* Small and hidden */
      opacity: 0; /* Transparent */
    }
    10% {
      transform: translateX(-50%) scale(1); /* Zoom to full size */
      opacity: 1; /* Fully visible */
    }
    80% {
      transform: translateX(-50%) scale(1); /* Stay full size for some time */
      opacity: 1; /* Fully visible */
    }
    100% {
      transform: translateX(-50%) scale(0.5); /* Shrink and disappear */
      opacity: 0; /* Transparent */
    }
  }

  /* Media query for mobile screens */
  @media screen and (max-width: 600px) {
    .popup-text {
      font-size: 14px; /* Smaller font for mobile */
      width: 95%; /* Wider popup for mobile */
      padding: 8px 12px; /* Adjust padding */
    }
  }
</style>

<script>
  // JavaScript to play the sound when the popup appears
  document.addEventListener("DOMContentLoaded", function() {
    const popupMessage = document.getElementById("popupMessage");
    const popupSound = document.getElementById("popupSound");

    // Function to play sound at the start of the animation
    popupMessage.addEventListener("animationstart", function() {
      popupSound.play();
    });
  });
</script>
