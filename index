<script>
// Function to show the "Show Your Call" modal
function showCallModal() {
    document.getElementById('callModal').style.display = 'block';
}

// Function to open the call spreadsheet link based on zone selection
function openCall(zone) {
    if (zone === 'Dhaka-1') {
        window.open('https://docs.google.com/spreadsheets/d/e/2PACX-1vQQ1xUgju54dAdR02J9h7FnHVI-oyyBtW1ciuySBde4bAu20xV2i-yv-EjoXtvc8JanDlDpa9sL2xHG/pubhtml?gid=0&single=true', '_blank');
    } else if (zone === 'Dhaka-2') {
        window.open('https://docs.google.com/spreadsheets/d/e/2PACX-1vSgJVLzmOnZw9NThCEw4XA-clymV_NLRlKOPNNCTsiJeZjW5AG4FPS4NKpg5yVmxqUjxVcWuHnba6il/pubhtml?gid=0&single=true', '_blank');
    }
    closeCallModal();
}

// Function to close the modal
function closeCallModal() {
    document.getElementById('callModal').style.display = 'none';
}
</script>

